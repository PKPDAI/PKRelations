{
  "run_name": "pkner-rex-dev-128-clean",
  "base_model": "data/models/pk-bert",

  "n_workers_dataloader": 12,
  "gpus": true,
  "seed": 1,

  "learning_rate": 3e-5,
  "max_epochs": 20,
  "max_length":128,
  "batch_size": 2,
  "val_batch_size": 2,
  "eps": 1e-8,
  "ctx_embedding_size": 4,

  "tensorboard_logger": true,
  "tag_type": "bio",
  "early_stopping": false,
  "early_stopping_patience": 5,

  "gradient_clip_val": 1.0,
  "weight_decay": 0.01,
  "stochastic_weight_avg": true,

  "remove_longer_seqs": true,
  "lr_warmup": false,
  "weighted_loss": false,
  "final_train": false,
  "include_ctx_emb": true,
  "include_cls": true
}
